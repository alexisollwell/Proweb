<p id="notice"><%= notice %></p>

<h1>Actividad Instructors</h1>

<table>
  <thead>
    <tr>
      <th>Actividadextra</th>
      <th>Instructor</th>
      <th>Horario</th>
      <th>Lugar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actividad_instructors.each do |actividad_instructor| %>
      <tr>
        <td><%= actividad_instructor.actividadExtra %></td>
        <td><%= actividad_instructor.instructor %></td>
        <td><%= actividad_instructor.horario %></td>
        <td><%= actividad_instructor.lugar %></td>
        <td><%= link_to 'Show', actividad_instructor %></td>
        <td><%= link_to 'Edit', edit_actividad_instructor_path(actividad_instructor) %></td>
        <td><%= link_to 'Destroy', actividad_instructor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actividad Instructor', new_actividad_instructor_path %>
